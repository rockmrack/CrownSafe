{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "FastAPI (uvicorn)",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "args": ["api.main_babyshield:app", "--host", "0.0.0.0", "--port", "8001", "--reload"],
      "jinja": true,
      "justMyCode": true
    },
    {
      "name": "Attach by Process",
      "type": "python",
      "request": "attach",
      "processId": "${command:python.pickProcess}"
    }
  ]
}
