{
  "dashboard": {
    "id": null,
    "title": "BabyShield • Chat (Phase-1 Canary)",
    "tags": ["babyshield", "chat", "canary"],
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "type": "stat",
        "title": "RPS",
        "gridPos": {"x": 0, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "bs:chat:rps",
            "legendFormat": "requests/sec",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "decimals": 2
          }
        }
      },
      {
        "id": 2,
        "type": "stat",
        "title": "Error Rate (conv)",
        "gridPos": {"x": 6, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "bs:chat:conv:error_rate",
            "legendFormat": "error_rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "decimals": 2,
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.01},
                {"color": "red", "value": 0.02}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "type": "stat",
        "title": "p95 Total Latency (ms)",
        "gridPos": {"x": 12, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "bs:chat:p95_total_ms",
            "legendFormat": "p95_ms",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "decimals": 0,
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 2000},
                {"color": "red", "value": 2800}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "type": "stat",
        "title": "Circuit Open Rate",
        "gridPos": {"x": 18, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "bs:chat:circuit_rate",
            "legendFormat": "circuit_rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "decimals": 3,
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.005},
                {"color": "red", "value": 0.01}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "type": "timeseries",
        "title": "Requests by Intent (5m rate)",
        "gridPos": {"x": 0, "y": 4, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum by (intent) (rate(bs_chat_requests_total{endpoint=\"conversation\"}[5m]))",
            "legendFormat": "{{intent}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "fillOpacity": 0.1,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "auto",
              "pointSize": 5,
              "stacking": {"mode": "none", "group": "A"},
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "scaleDistribution": {"type": "linear"},
              "axisCenteredZero": false,
              "hideFrom": {"legend": false, "tooltip": false, "vis": false},
              "thresholdsStyle": {"mode": "off"}
            }
          }
        }
      },
      {
        "id": 6,
        "type": "timeseries",
        "title": "Error vs Success",
        "gridPos": {"x": 12, "y": 4, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum(rate(bs_chat_requests_total{ok=\"0\"}[5m]))",
            "legendFormat": "errors",
            "refId": "A"
          },
          {
            "expr": "sum(rate(bs_chat_requests_total{ok=\"1\"}[5m]))",
            "legendFormat": "success",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "fillOpacity": 0.1,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "auto",
              "pointSize": 5,
              "stacking": {"mode": "none", "group": "A"},
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "scaleDistribution": {"type": "linear"},
              "axisCenteredZero": false,
              "hideFrom": {"legend": false, "tooltip": false, "vis": false},
              "thresholdsStyle": {"mode": "off"}
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "errors"},
              "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "red"}}]
            },
            {
              "matcher": {"id": "byName", "options": "success"},
              "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "green"}}]
            }
          ]
        }
      },
      {
        "id": 7,
        "type": "timeseries",
        "title": "p95 Tool Latency (ms) by Intent",
        "gridPos": {"x": 0, "y": 12, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum by (intent,le) (rate(bs_chat_tool_latency_ms_bucket[5m])))",
            "legendFormat": "{{intent}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "fillOpacity": 0.1,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "auto",
              "pointSize": 5,
              "stacking": {"mode": "none", "group": "A"},
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "scaleDistribution": {"type": "linear"},
              "axisCenteredZero": false,
              "hideFrom": {"legend": false, "tooltip": false, "vis": false},
              "thresholdsStyle": {"mode": "off"}
            }
          }
        }
      },
      {
        "id": 8,
        "type": "timeseries",
        "title": "p95 Synth Latency (ms)",
        "gridPos": {"x": 12, "y": 12, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum by (le) (rate(bs_chat_synth_latency_ms_bucket[5m])))",
            "legendFormat": "p95_synth_ms",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "fillOpacity": 0.1,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "auto",
              "pointSize": 5,
              "stacking": {"mode": "none", "group": "A"},
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "scaleDistribution": {"type": "linear"},
              "axisCenteredZero": false,
              "hideFrom": {"legend": false, "tooltip": false, "vis": false},
              "thresholdsStyle": {"mode": "off"}
            }
          }
        }
      },
      {
        "id": 9,
        "type": "timeseries",
        "title": "Fallback & Blocked Rates",
        "gridPos": {"x": 0, "y": 20, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "bs:chat:fallback_rate",
            "legendFormat": "fallback_rate",
            "refId": "A"
          },
          {
            "expr": "bs:chat:blocked_rate",
            "legendFormat": "blocked_rate",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "fillOpacity": 0.1,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "auto",
              "pointSize": 5,
              "stacking": {"mode": "none", "group": "A"},
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "scaleDistribution": {"type": "linear"},
              "axisCenteredZero": false,
              "hideFrom": {"legend": false, "tooltip": false, "vis": false},
              "thresholdsStyle": {"mode": "off"}
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "fallback_rate"},
              "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "orange"}}]
            },
            {
              "matcher": {"id": "byName", "options": "blocked_rate"},
              "properties": [{"id": "color", "value": {"mode": "fixed", "fixedColor": "blue"}}]
            }
          ]
        }
      },
      {
        "id": 10,
        "type": "table",
        "title": "Tool Calls — Last 15m (by Intent)",
        "gridPos": {"x": 12, "y": 20, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum by (intent) (rate(bs_chat_requests_total{endpoint=\"conversation\"}[15m]))",
            "legendFormat": "{{intent}}",
            "refId": "A",
            "format": "table"
          },
          {
            "expr": "histogram_quantile(0.95, sum by (intent,le) (rate(bs_chat_tool_latency_ms_bucket[15m])))",
            "legendFormat": "{{intent}}_p95",
            "refId": "B",
            "format": "table"
          }
        ],
        "transformations": [
          {
            "id": "merge",
            "options": {}
          },
          {
            "id": "organize",
            "options": {
              "excludeByName": {"Time": true},
              "indexByName": {},
              "renameByName": {
                "Value #A": "RPS",
                "Value #B": "p95 (ms)"
              }
            }
          }
        ],
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "cellOptions": {
                "type": "auto"
              },
              "inspect": false
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "RPS"},
              "properties": [
                {"id": "unit", "value": "reqps"},
                {"id": "decimals", "value": 3}
              ]
            },
            {
              "matcher": {"id": "byName", "options": "p95 (ms)"},
              "properties": [
                {"id": "unit", "value": "ms"},
                {"id": "decimals", "value": 0}
              ]
            }
          ]
        }
      }
    ],
    "schemaVersion": 39,
    "version": 1
  }
}
